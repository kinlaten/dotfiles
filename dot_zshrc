function y() {
  local tmp=$(mktemp -t "yazi-cwd.XXXXXX")
  yazi "$@" --cwd-file="$tmp"
  local cwd
  cwd=$(cat "$tmp")
  rm -f -- "$tmp"
  if [ -n "$cwd" ] && [ "$cwd" != "$PWD" ]
  then
    builtin cd "$cwd"
  fi
}

